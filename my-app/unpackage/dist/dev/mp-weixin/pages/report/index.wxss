.uni-media-list-body {
  height: auto;
}
.uni-media-list-text-top {
  line-height: 1.6em;
}
._div {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.wuc-tab-item {
  width: 30%;
  text-align: center;
}
.home-header {
  height: 100px;
  background-color: #ffffff;
  margin: 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.home-header .btn {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.home-header .btn1 {
  background-color: #CCCCFF;
}
.home-header .btn2 {
  background-color: #CCFFCC;
}
.home-header .btn3 {
  background-color: #FF88C2;
}
@font-face {
  font-family: texticons;
  font-weight: normal;
  font-style: normal;
  src: url("//at.alicdn.com/t/font_1801650_kjk887fh3yk.ttf") format("truetype");
}
.center-list {
  background-color: #FFFFFF;
  margin-top: 20rpx;
  width: 750rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.center-list-item {
  height: 100rpx;
  width: 750rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  padding: 0rpx 20rpx;
}
.border-bottom {
  border-bottom-width: 1rpx;
  border-color: #c8c7cc;
  border-bottom-style: solid;
}
.list-icon {
  height: 100rpx;
  line-height: 100rpx;
  width: 40rpx;
  font-size: 40rpx;
  color: #898989;
  text-align: center;
  font-family: texticons;
  margin-right: 30rpx;
}
.list-text {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 34rpx;
  color: #555;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: left;
}
.navigat-arrow {
  height: 100rpx;
  line-height: 100rpx;
  width: 40rpx;
  font-size: 34rpx;
  color: #555;
  text-align: right;
  font-family: texticons;
  float: right;
}
.content > .center-list:nth-child(1) .list-icon,
.content > .center-list:nth-child(5) .list-icon {
  color: #FA9D3B;
}
.content > .center-list:nth-child(2) .list-icon,
.content > .center-list:nth-child(6) .list-icon {
  color: #1677D2;
}
.content > .center-list:nth-child(3) .list-icon,
.content > .center-list:nth-child(7) .list-icon {
  color: #20C5C1;
}
.content > .center-list:nth-child(4) .list-icon,
.content > .center-list:nth-child(8) .list-icon {
  color: #FE0021;
}

